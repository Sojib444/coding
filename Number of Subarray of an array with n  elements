#include<bits/stdc++.h>
using namespace std;
int main()
{

    int n,i,j,curr=0;
    cin>>n;
    int arr[n];
    for(i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    for(i=0;i<n;i++)
    {
        curr=0;
        for(j=i;j<n;j++)
        {
            curr+=arr[j];
            cout<<curr<<endl;
        }
    }

    return 0;
}
